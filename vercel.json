{
  "functions": {
    "app/api/pagespeed/route.ts": {
      "maxDuration": 50
    },
    "app/api/pagespeed/refresh/route.ts": {
      "maxDuration": 300
    }
  },
  "headers": [
    {
      "source": "/api/pagespeed",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=43200, stale-while-revalidate=86400"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "max-age=43200"
        }
      ]
    }
  ]
}
