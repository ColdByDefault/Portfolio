  "private": true,
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0 --turbopack",
    "build": "next build --webpack",
    "start": "next start --hostname 0.0.0.0",
    "deploy": "prisma migrate deploy && npm run build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test-dep": "npx npm-check-updates --interactive",
    "postinstall": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "db:test": "tsx test-blogs.ts",
    "docs": "npx typedoc --entryPointStrategy expand --entryPoints components app types hooks data i18n lib mcp-server messages public styles --tsconfig tsconfig.json --out docs"
  },
